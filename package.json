    "@qbcart/types": "^1.0.0",
    "@qbcart/utils": "^1.0.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@types/bootstrap": "^4.6.0",
    "@types/jquery": "^3.5.5",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy": "^1.18.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "snowpack": "^3.1.2",
    "typescript": "^4.2.2"
  },
  "husky": {
    "hooks": {
