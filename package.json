    "lint": "eslint src/**/*.tsx",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "echo \"All Good\" && exit 0"
  },
  "dependencies": {},
  "devDependencies": {
    "@qbcart/eshop-local-db": "^1.0.0-beta.1",
    "@qbcart/eshop-product-modal": "^1.0.1",
