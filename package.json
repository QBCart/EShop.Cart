  "exports": {
    "import": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QBCart/EShop.Cart.git"
  },
  "bugs": {
    "url": "https://github.com/QBCart/EShop.Cart/issues"
  },
  "homepage": "https://github.com/QBCart/EShop.Cart#readme",
  "scripts": {
    "start": "snowpack dev --config snowpack.dev.config.js",
    "start:proxy": "snowpack dev --config snowpack.proxy.config.js",
